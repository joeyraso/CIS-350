<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#ffffffff">


    <ListView
        android:layout_width="wrap_content"
        android:layout_height="395dp"
        android:id="@+id/list"
        android:layout_gravity="left|center_vertical"
        android:choiceMode="singleChoice"
        android:headerDividersEnabled="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/newJobsAroundYou"
        android:id="@+id/textView"
        android:layout_gravity="center_horizontal|top"
        android:textColor="#ffffffff"
        android:typeface="sans"
        android:background="#1E89FF"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:singleLine="false" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:id="@+id/profileButton"
        android:onClick="displayProfile"
        android:layout_gravity="bottom" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="193dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/textView7"
        android:layout_gravity="left|center_vertical" />

    <Button
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="@string/addTab"
        android:id="@+id/addJobButton"
        android:onClick="addJob"
        android:layout_gravity="top|right" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dashboardTab"
        android:id="@+id/signupButton"
        android:layout_gravity="center_horizontal|bottom" />

    <Button
        android:layout_width="74dp"
        android:layout_height="wrap_content"
        android:text="@string/cartTab"
        android:id="@+id/button3"
        android:onClick="displayCart"
        android:layout_gravity="bottom|right" />

</FrameLayout>
