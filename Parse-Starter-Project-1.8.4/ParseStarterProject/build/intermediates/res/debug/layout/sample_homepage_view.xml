<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.parse.starter"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <com.parse.starter.HomepageView android:background="#ffffffff" android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="20dp" android:paddingBottom="40dp"
        app:exampleDimension="24sp" app:exampleColor="#33b5e5"
        app:exampleString="Hello, HomepageView"
        app:exampleDrawable="@android:drawable/ic_menu_add"
        android:layout_gravity="left|top" />


    <ListView
        android:layout_width="wrap_content"
        android:layout_height="395dp"
        android:id="@+id/listView"
        android:layout_gravity="left|center_vertical"
        android:choiceMode="singleChoice" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="New Jobs Around You"
        android:id="@+id/textView"
        android:layout_gravity="center_horizontal|top"
        android:textColor="#ff1e89ff"
        android:typeface="sans" />

    <Button
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="Cart"
        android:id="@+id/button3"
        android:layout_gravity="center_horizontal|bottom" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:id="@+id/button2"
        android:layout_gravity="right|bottom" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dashboard"
        android:id="@+id/button"
        android:layout_gravity="left|bottom" />

</FrameLayout>
